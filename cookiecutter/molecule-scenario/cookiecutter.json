{
    "name": "default",
    "os": ["centos7", "centos8", "debian9", "debian10"],
    "driver": ["vagrant", "delegated"],
    "driver_provider": "",
    "driver_libvirt_host": "localhost",
    "driver_libvirt_port": "22",
    "driver_libvirt_uri": "qemu:///system",
    "driver_libvirt_prepare": "",
    "driver_libvirt_logfile": "~/.cache/virt-up/virt-up.log",
    "driver_libvirt_loglevel": "info",
    "driver_libvirt_python_interpreter": "/usr/bin/python3",
    "number_instances": 1,
    "instance_name": "instance",
    "instance_mem": 4096,
    "instance_cpus": 1,
    "update_kernel": "no",
    "selinux_mode": ["permissive", "disabled", "enforcing"],
    "im": ["default", "managed", "packages", "bdist", "scm"],
    "im_module": ["dkms", "kmod"],
    "build": ["none", "bdist", "rpms"],
    "bdist_flavor": ["modern", "transarc"],
    "service_keytab": "files/example-aes.keytab",
    "check_dependency": "no",
    "check_idempotence": "no",
    "check_side_effect": "no",
    "groups": "",
    "with_devel": "no",
    "with_krbclient": "no",
    "with_krbserver": "no",
    "with_common": "no",
    "with_client": "no",
    "with_server": "no",
    "with_cell": "no",
    "with_robotest": "no",
    "with_playbooks": "",
    "_copy_without_render": [
        "files/*",
        "library/*"
    ]
}
